{
  "expo": {
    "name": "MyUniversity",
    "slug": "uni-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/appstore.png",
    "splash": {
      "image": "./app/assets/RIU.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "tothepointcode.unvierstyapp",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/appstore.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/appstore.png"
    }
  }
}
